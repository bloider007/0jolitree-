<script type="text/javascript">
function init() {
	<?php
	for($i=1;$i<25; $i++)
	{
	?>
	var tree<?=$i?> = new JoliTree(document.getElementById("myTree<?=$i?>"));		
	<?		
	}
	?>
}

window.onload = init;
</script>
